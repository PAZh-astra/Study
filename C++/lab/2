//2.1
#include <iostream>
using namespace std;
 
int main() {
int n, b, a;
cin >> n >> b;
a=n%b;
if (b-a>a) n-=(b-a);
else n+=(b-a);
cout << n;
	return 0;
}


//2.2
#include <iostream>
#include <math.h>
using namespace std;
 
int main() {
int a, b;
cin >> a >> b;
cout << sqrt(a*a+b*b);
	return 0;
}


//2.3
#include <iostream>
using namespace std;
 
int main() {
	int abc, a, b, c;
	cin >> abc;
	a=abc/100;
	b=abc/10-a*10;
	c=abc%10;
	cout << a+b+c << endl << a*b*c;
	return 0;
}


//2.4
#include <iostream>
using namespace std;
 
int main() {
int x1, y1, x2, y2, k, b, x, y;
cin >> x1 >> y1 >> x2 >> y2;
//y=kx+b
k=(y2-y1)/(x2-x1);
b=y2-k*x2;
cout << "y="<< k << "x+"<< b;
	return 0;
}


//2.5
#include <iostream>
#include <math.h>
using namespace std;
 
int main() {
float m, s, n, y, p;
cin >> n >> y >> p;
p/=100;
m = (n * p * pow((1+p),y)) / (12 * (pow(1+p,y) - 1));
s=m*y*12;
cout << m << endl << s;
	return 0;
}


//2.6
#include <iostream>
using namespace std;
 
int main() {
int a, b;
cin >> a >> b;
b+=a;
a=b-a;
b=b-a;
cout << a << endl << b;
	return 0;
}


//2.7
