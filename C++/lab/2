}


//2.9(c)
#include <iostream>
using namespace std;

int main() {
float a, b, c, d;
cin >> a >> b >> c >> d;
int P = a+b+c+d;
cout << "P= " << P << endl;
return 0;
}


//2.10
#include <iostream>
#include <math.h>
using namespace std;
int main() {
float r;
cin >>  r;
const double PI = acos(-1.0);
int V = (4/3)*PI*r*r*r;
int Sp = 4*PI*r*r;
cout << "V= " <<  V << endl << "Sp="<< Sp« endl;
return 0;
}


//2.11
#include <iostream>
#include <math.h>
using namespace std;
int main() {
float r, h;
cin >> r >> h;
const double PI = acos(-1.0);
int V = PI*r*r*h;
int S1= 2*PI*r*h;
int S2= PI*r*r;
int Sp = S1+2*S2;
cout << "V= " << V << endl << "Sp="<< Sp« endl;
return 0;
}


//2.12
#include <iostream>
#include <math.h>
using namespace std;
int main() {
float r, l;
cin >> r >> l;
int h= l*l-r*r;
h= sqrt(h);
int Sp = r*h;
cout << "Sp=" << Sp << endl;
return 0;
}


//2.13
#include <iostream>
#include <math.h>
using namespace std;
int main() {
float r, h;
cin >> r >> h;
int l = h*h+r*r;
h= sqrt(l);
const double PI = acos(-1.0);
int V = (PI*r*r*h)/3;
int Sp = PI*r*(r+l);
cout << "V= " << V << endl << "Sp="<< Sp<< endl;
return 0;
}


//2.14
#include <iostream>
#include <math.h>
using namespace std;
int main() {
float a, h;
cin >> a >> h;
int V = (a*a*h)/(4*sqrt(3));
int Sp = a*(a*sqrt(3)+6*h);
cout << "V= " << V << endl << "Sp="<< Sp<< endl;
return 0;
}


//2.15
#include <iostream>
#include <math.h>
using namespace std;
int main() {
int a, h, c, l;
cin >> a >> h;
c = h*h+a*a;
c = sqrt (c);
l=c*c-(a*a/4);
l = sqrt (l);
int V = (a*a*h)*sqrt(3)/2;
int Sp = 3*a*(a*sqrt(3)/2+l);
cout << "V= " << V << endl << "Sp="<< Sp << endl;
return 0;
}
