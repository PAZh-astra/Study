int main() {
int abcd, ab, cd;
cin >> abcd;
ab=abcd/100;
cd=abcd%100;
cout << ab+cd;
	return 0;
}


//2.8
#include <iostream>
#include <math.h>
using namespace std;
 
int main() {
int r, h, s, v, c;
cin >> r >> h >> v >> c;
//asin(0)=arcsin(0)=pi
s=asin(0)*r*r+2*r*h*asin(0);
cout << ceil(v/c);
	return 0;
}


